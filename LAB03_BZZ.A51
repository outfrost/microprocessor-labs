		LJMP START
		ORG 0x0100
		
START:	MOV R4, #0x02
GO:		MOV R2, #0x10

CYCLE:	CPL P3.2
		
		MOV A, R4
		MOV R0, A
		MOV R1, #0x40
DELAY0:	DJNZ R1, DELAY0
		DJNZ R0, DELAY0

		CPL P3.2
		
		MOV A, R4
		MOV R0, A
		MOV R1, #0x40
DELAY1:	DJNZ R1, DELAY1
		DJNZ R0, DELAY1

		DJNZ R2, CYCLE
		MOV A, R4
		CPL A
		XRL A, #0xFA
		MOV R4, A
		JMP GO

		NOP
		NOP
		NOP
		JMP $
END START